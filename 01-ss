#!/bin/bash
uname -snrvm
# 颜色
CYAN="\033[1;36m"
GREEN="\033[1;32m"
YELLOW="\033[1;33m"
RESET="\033[0m"

# ASCII LOGO
echo -e "${CYAN}"
echo "   _____ __              __                              __       "
echo "  / ___// /_  ____ _____/ /___ _      ___________  _____/ /_______"
echo "  \__ \/ __ \/ __ \`/ __  / __ \ | /| / / ___/ __ \/ ___/ //_/ ___/"
echo " ___/ / / / / /_/ / /_/ / /_/ / |/ |/ (__  ) /_/ / /__/ ,< (__  ) "
echo "/____/_/ /_/\__,_/\__,_/\____/|__/|__/____/\____/\___/_/|_/____/ "
echo -e "${RESET}"
