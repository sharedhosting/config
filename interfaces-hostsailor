source /etc/network/interfaces.d/*

auto lo
iface lo inet loopback

auto ens3
iface ens3 inet dhcp

iface ens3 inet6 static
    address 2a0e:df40:3:16::2/64
    address 2a0e:df40:3:16::a/64
    address 2a0e:df40:3:16::16/64
    address 2a0e:df40:3:16::aff/64
    gateway 2a0e:df40:3::1
    accept_ra 0
